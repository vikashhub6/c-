// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std ;
int main() {

int array [ 6]= {1,2,4,6,7,8,};
  
  
for ( int i = 1 ; i < 9 ; i++)
 {  bool bollean = false ;
  
     // this is one method for having one work ata a time in aa loop 
     for (int c = 0 ; c <6; c++)
       {
           if ( array[c]==i)
           { bollean = true ;
                break ; }
       }
     
     if (!bollean)
      {cout << i ;
      return 0; }
 }


    return 0;
}
