
         
         
// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std ;

int main() {
 int array [5][5];
  cout << " enter the value of array ; ";
    for (int i = 0 ; i<5; i++)
     { for (int j=0 ; j<5; j++)
         {  cin>> array[i][j] ;}
     }

 int target; 
 cout << " enter the value of target ";
 cin>> target;
 
  // first time binary concept purlu apply throught the 2 d array but //its not noramla array it quite diffrent  although it  can be  //applied to normal array because if it can solve diificult type //of array it means it can solve easey one too . 
    int i = 0 , j = 4 ; 
    while (i<5&& j>=0)
    {if (array[i][j] == target )
      {cout << " find ";
       return 1 ; }
       
     else if ( array[i][j]> target)
       { j--;}
      else 
       {  i++;}
        
    }
       
      cout << " not found ";
      
          
      
   
         
     
         
     


         
     


    return 0;
}
