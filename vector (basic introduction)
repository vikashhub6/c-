        // sorting in vector 
    vector<int>ans ;
    ans.push_back (7);
    ans.push_back (9);
    ans.push_back(6);
    ans.push_back (76);
    sort(ans.begin(),ans.end()) ; // its for increasing order 
    sort (ans.begin(), ans.end(), greater<int>() ) ;  // its for decresing order
    sort (ans.rbegin(),ans.rend()) ;// its for decresing order 
    for ( int i= 0 ; i < ans.size() ; i ++)
      cout << ans[i] << " " ;
       // serch in binary serch 
      cout << binary_search(ans.begin(), ans.end() , 9 );
      // give index of particular element in element
      cout << find(ans.begin(), ans.end() , 6)- ans.begin() << endl;
