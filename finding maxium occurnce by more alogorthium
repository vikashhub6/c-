// Online C++ compiler to run C++ program online
#include <iostream>

using namespace std ;
// more  voting alogorthium 
int main() {
    int array[11]= {3,3,2,1,3,5,5,5,5,5,5 };
     int candidate= array[0];
      int count = 1 ;
      for ( int c = 1 ; c< 11 ; c++)
        { 
            if (array[c]==candidate)
            count++;
            if (array[c]!=candidate)
            count--;
            if (count==0)
             {candidate = array[c+1];
              count = 1;}
            
        }
 cout << " winner" << candidate ;
    return 0;
}
