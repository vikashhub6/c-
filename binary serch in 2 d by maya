int main() {
 int array [4][4];
  cout << " enter the value of array ; ";
    for (int i = 0 ; i<4; i++)
     { for (int j=0 ; j<4 ; j++)
         {  cin>> array[i][j] ;}
     }

 int target; 
 cout << " enter the value of target ";
 cin>> target;
 
  // nakli 1 d array creation 
  
   int start = 0 , end = 3 ;
    while (start<=end )
    {int mid = start+end/2 ;
     int row_index= mid /4;
      int column_index= mid%4;
      if ( array[row_index][column_index] ==target)
         { cout << " find " ;
           return 1 ; }
       else if( array[row_index][column_index]<target)
          {start = mid+1;}
          
          else
          { end =mid-1;}
              
          
        
          
         }
       
cout << " no";
              
             
           
        
              
         
            
         
         
             
         
    
   
   
   
     
         
     
         
     


