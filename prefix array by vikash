// Online C++ compiler to run C++ program online
#include <iostream>
 using namespace std ;

int main() {
int array[1000] ={6,4,5, -3, 2,8};
 int sum = 0 ;
 int prefixArray[6];
for (int i =0 ;  i < 6 ; i ++)
 { sum += array[i];
    for (int a = i ; a <6; a ++)
     { prefixArray[a]= sum ;
        break;
     }
}

  for (int s = 0 ; s<6 ; s++)
   cout <<prefixArray [s] <<  " " ;

    return 0;
}
