#include <iostream>
using  namespace std ; 

int main() {
     int array[4][4];
     cout << "enter the element of array " ;
      
      for ( int i = 0 ; i <4; i++)
        { for (int j=0 ; j< 4; j++)
            cin >> array[i][j] ; 
        }
     
     int first_diagonal = 0 ;
     int second_diagonal = 0;
     
     for ( int i = 0 ; i < 4 ;i++ )
        {first_diagonal += array[i][i] ;}
        
        int j = 3;
         for(int i = 0 ; i < 4 ; i++)
          {second_diagonal += array[i][j];
           j--  ; 
          }
              
             
     cout << " second_ diagonal sum ="<< second_diagonal;
     
              
          
        
        
      
      
    cout << " vikash answer given is uniqe=" << first_diagonal;
          
      
     
        
 

    return 0;
}
