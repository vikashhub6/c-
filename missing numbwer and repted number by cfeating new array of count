// Online C++ compiler to run C++ program online
#include <iostream>
#include<vector>
using namespace std ;
int main() {
   int array [7]= {1,2,3,4,5,5,6};
   vector<int>count(8,0);
   for ( int c = 0 ; c < 7; c++)
     { int d = array[c];
       count[d]+=1 ;}
       
       for ( int d = 1; d<8; d++)
         { if(count [d]==2)
            cout << " repeted ="<< d << endl ;
            else if (count[d]==0)
              cout << " missied="<< d;
         }
         
     
   
   
   
    return 0;
}
