#include <iostream>
using  namespace std ;     

int main() {
     int array[4][4];
     cout << "enter the element of array " ;
      
      for ( int i = 0 ; i <4; i++)
        { for (int j=0 ; j< 4; j++)
            cin >> array[i][j] ; 
        }
        
        
          int  rotate [4][4];
         for ( int i = 0 ; i <4; i++)
        {   int k = 3 ;
            for (int j=0 ; j< 4; j++)
            {  rotate[i][j]= array[k][i];
                k--;
            }
        }
      
      for ( int i = 0 ; i <4; i++)
        { for (int j=0 ; j< 4; j++)
            cout<< rotate[i][j] <<" " ; 
        }
        
 

    return 0;
}
