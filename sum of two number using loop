// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std ;

int main()
 
    // sum of two number {all possible case }
{
    
      int size;
      cout << " enter te value of size " ;
      cin >> size ;
    int array [1000];
    cout << " enter the number of which array has to be formed";
    int first = -1 , second = -1 ;
    for (int i = 0 ; i < size; i ++  )
      {cin >> array[i] ;}
    int  target  ;
    cout << "enter the value of target ";
    cin >> target ;
    for ( int i = 0 ; i < size-1; i ++)
    {  for (int c = i+1 ; c < size ; c++)
        { if( array[i]+ array [c] == target)
         { first = array[i];
          second= array[c];
          cout << first <<" "<< second  << endl;
          break ;}
        }
    }
        

    return 0;
}
      
           
    

    
